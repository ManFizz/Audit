<Window x:Class="Audit.LogInWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Audit"
        mc:Ignorable="d"
        Title="Log in account" Height="170" Width="300">
    <StackPanel Margin="10">
        <TextBlock TextAlignment="Center">Login</TextBlock>
        <TextBox x:Name="Login" Width="250">Amogus77</TextBox>
        <TextBlock TextAlignment="Center">Password</TextBlock>
        <PasswordBox x:Name="Password" Width="250" Password="123456"/>
        <Button Click="LogInButton_Click"  Width="80" Margin="0 12 0 0">Sign in</Button>
    </StackPanel>
</Window>
